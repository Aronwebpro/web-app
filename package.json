{
  "name": "web-app",
  "private": true,
  "scripts": {
    "start": "MONGO_URL=mongodb://localhost:27017/web-app meteor run -p3001 --settings=.deploy/settings.json",
    "start-mongodb": "brew services start mongodb-community",
    "stop-mongodb": "brew services stop mongodb-community",
    "w-start": "meteor run -p3001 --settings=.deploy/settings.json",
    "w-start-mongo": "set MONGO_URL=mongodb://localhost:27017/web-app",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@types/meteor": "^1.4.50",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "antd": "^4.7.0",
    "aphrodite": "^2.4.0",
    "bcrypt": "^5.0.0",
    "d3": "^7.6.1",
    "hoist-non-react-statics": "^3.3.2",
    "indexof": "0.0.1",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.12.2",
    "meteor-node-stubs": "^1.0.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.5",
    "react": "^16.13.1",
    "react-dates": "^21.8.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.27.11",
    "react-with-direction": "^1.3.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "simpl-schema": "^1.10.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.21.4"
  }
}
